#include <bits/stdc++.h>
using namespace std;

int main()
{
int T;
cin>>T;
while(T--)
    {
    int a,b;
    cin>>a>>b;
    int a1=0,a2=0,b1=0,b2=0;
    a1=a/10;
    a2=a%10;
    b1=b/10;
    b2=b%10;
    int sum=0;
    if(a1==0)
    {
        if(b1==0)
            sum=a+b;
        else
        {
            sum=max(b1*10+b2+a2,a2*10+b2+b1);
        }
    }
    else if(b1==0)
    {
        if(a1==0)
            sum=a+b;
        else
        {
            sum=max(a1*10+b2+a2,b2*10+a2+a1);
        }
    }
    else
    {
        sum=max(a1*10+a2+b1*10+b2,max(b2*10+a2+b1*10+a1,a2*10+b2+a1*10+b1));
    }
        cout<<sum<<endl;
}
return 0;
}
