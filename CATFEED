#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t!=0){
	    int n,m;
	    cin>>n>>m;
	    int i,ans=0,j;
	    int b[n],a[m];
	    for(i=0;i<m;i++)
	    cin>>a[i];
	    for(i=1;i<=n;i++)
	    b[i]=0;
	    for(i=0;i<m;i++){
	        b[a[i]]++;
	        for(j=1;j<=n;j++){
	            if(b[a[i]]-b[j]>1){
	                ans=1;
	                break;
	                
	            }
	        }
	        if(ans==1)
	        break;
	    }
	    if(ans==0)
	    cout<<"YES"<<endl;
	    else
	    cout<<"NO"<<endl;
	    t--;
	    
	}
	return 0;
}
