#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	while(T--)
	{
	    int N;
	    cin>>N;
	    int count=0;
	    for(int i=1;i<=N;i++)
	        if(N%i==0)
	            count++;
	   if(count==2)
	        cout<<"yes"<<endl;
	   else
	        cout<<"no"<<endl;
	}
	return 0;
}
