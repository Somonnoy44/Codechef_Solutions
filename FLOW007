#include <iostream>

#include <cstdlib>
using namespace std;

int main()
{
        int T;
        cin>>T;
        while(T--)
        {
        	int N;
        	cin>>N;
        	int rev=0;
        	for(int i=N;i>0;i=i/10)
        	{
        		int d=i%10;
        		rev=(rev*10)+d;
			}
			cout<<rev<<endl;
		}
}
