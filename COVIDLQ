#include <bits/stdc++.h> 
#include <iostream>
#include <iostream>
#include <cstdlib>
using namespace std; 
int main()
{
       int T;
       cin>>T;
       while(T--)
       {
       	int N;
       	cin>>N;
       	int A[N];
       	int flag=1;
       	int distance=6;
       	for(int i=0;i<N;i++)
			cin>>A[i];
        for(int i=0;i<N;i++)
        {
        	distance++;
			if(A[i]==1)
			{
				if(distance>=6)
				{
					distance=0;
					continue;
				}
				else
				{
						flag=0;
						break;
				}
			}
		}
		if(flag==1)
        	cout<<"YES"<<endl;
        else
        	cout<<"NO"<<endl; 
	 } 
}

