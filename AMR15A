#include <iostream>
using namespace std;

int main()
{
       int N;
       cin>>N;
       int A[N];
       int oddweapons=0;
       int evenweapons=0;
       for(int i=0;i<N;i++)
       		cin>>A[i];
    	
		for(int i=0;i<N;i++)
		{
			if(A[i]%2==0)
				evenweapons++;
			else
				oddweapons++;
		}
		if(evenweapons>oddweapons)
			cout<<"READY FOR BATTLE";
		else
			cout<<"NOT READY";
}
